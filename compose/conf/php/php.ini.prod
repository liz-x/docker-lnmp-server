[PHP]

; 基础配置
short_open_tag = On
date.timezone = PRC
; 脚本的最大执行时间 单位：秒
max_execution_time = 300

; 上传配置
upload_max_filesize = 20M
max_file_uploads = 20

; 异常抛出
error_reporting = E_ALL & ~E_DEPRECATED & ~E_STRICT
display_errors = Off
display_startup_errors = Off

; 日志
log_errors = On
log_errors_max_len = 1024
error_log = /websvr/log/php/php-error.log
